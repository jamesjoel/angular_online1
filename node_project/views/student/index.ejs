<h2 class="text-center">Student page</h2>
<table class="table table-dark table-stripped">
    <tr>
        <th>S.No.</th>
        <th>Name</th>
        <th>Age</th>
    </tr>
    <% result.forEach(function(row, index){ %>
        <tr>
            <td><%= index+1 %></td>
            <td><%= row.name %></td>
            <td><%= row.age %></td>
        </tr>
    <% }) %>
</table>